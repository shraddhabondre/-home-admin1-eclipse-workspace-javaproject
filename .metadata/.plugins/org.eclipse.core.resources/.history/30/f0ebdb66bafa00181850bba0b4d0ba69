package firstweek;

public class PrintThreeNames 
{
	public static void main(String args[])
    {

		String str = "I live in India";
		String result = "";
		String[] words = str.split(" ");
		for (int i=words.length-1;i>=0;i--)
		{
		    result = result + words[i] + " ";
		}
		System.out.println(result);
		
}
}
